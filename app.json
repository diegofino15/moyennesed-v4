{
  "expo": {
    "name": "MoyennesED Sans Pubs",
    "slug": "moyennesed-no-ads",
    "version": "4.0.20",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "9eb7da33-9edc-4dfa-93d8-f88a7ee8e932"
      }
    },
    "owner": "finocchiarodev",
    "plugins": [
      "./src/util/firebase/customAppCheck.js",
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "buildReactNativeFromSource": true
          }
        }
      ],
      "expo-font",
      [
        "react-native-permissions",
        {
          "iosPermissions": [
            "Notifications"
          ]
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "me.diegof.moyennesed-no-ads",
      "buildNumber": "1",
      "userInterfaceStyle": "automatic",
      "googleServicesFile": "./GoogleService-Info.plist",
      "entitlements": {
        "aps-environment": "production"
      }
    },
    "android": {
      "package": "com.finocchiarodev.moyennesednoads",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 1,
      "userInterfaceStyle": "automatic",
      "googleServicesFile": "./google-services.json"
    }
  }
}
